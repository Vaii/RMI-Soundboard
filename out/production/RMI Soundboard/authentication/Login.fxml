<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="authentication.LoginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="341.0" prefWidth="421.0">
         <children>
            <TextField fx:id="username" layoutX="127.0" layoutY="102.0" />
            <TextField fx:id="password" layoutX="127.0" layoutY="144.0" />
            <Label layoutX="64.0" layoutY="107.0" text="Username" />
            <Label layoutX="64.0" layoutY="149.0" text="Password" />
            <Label fx:id="noAccount" layoutX="138.0" layoutY="216.0" text="No Account? Click here!" textFill="#001aff" underline="true" />
            <Button fx:id="login" layoutX="128.0" layoutY="179.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="167.0" text="Login" />
            <RadioButton fx:id="controller" layoutX="128.0" layoutY="76.0" mnemonicParsing="false" text="Controller" />
            <RadioButton fx:id="client" layoutX="234.0" layoutY="76.0" mnemonicParsing="false" text="Client" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
